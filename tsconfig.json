{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./renderer",
    "esModuleInterop": true,
    "jsx": "react",
    "lib": ["es7", "dom"],
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "pretty": true,
    "rootDir": "./renderer",
    "sourceMap": true,
    "strict": true,
    "target": "es6"
  },
  "exclude": ["node_modules"],
  "include": ["./renderer/**/*"]
}
