{
  "extends": "./node_modules/electron-webpack/tsconfig-base.json",
  "compilerOptions": {
    "jsx": "react",

    "baseUrl": "./src",
    "paths": {
      "common/*": ["common/*"],
      "@/*": ["main/*", "renderer/*"]
    }
  },
  "include": [
    "src/main/**/*",
    "src/renderer/**/*",
    "src/common/**/*"
  ]
}
