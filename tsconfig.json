{
  "compilerOptions": {
    "baseUrl": "./renderer/",
    "jsx": "react",
    "lib": ["es7", "dom"],
    "module": "commonjs",
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "sourceMap": true,
    "strict": true,
    "target": "es6"
  },
  "exclude": ["node_modules"],
  "include": ["./renderer/**/*"]
}
